package com.Flipkart;
import java.util.*;

public class Paramaterised_Con {
	
	String color;
	int cost;
	public Paramaterised_Con(String color,int cost) {
		this.color=color;
		this.cost=cost;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the 1st Color:");
		String S=sc.next();	
		System.out.println("Enter the Cost:");
		int c=sc.nextInt();
		Paramaterised_Con p1=new Paramaterised_Con(S , c);
		System.out.println(p1.color + " " + p1.cost);
		
		System.out.println("Enter the 2nd Color:");
		String S1=sc.next();	
		System.out.println("Enter the cost:");
		int c1=sc.nextInt();
		Paramaterised_Con p2=new Paramaterised_Con(S1,c1);
		System.out.println(p2.color+ " " + p1.cost);
	}

}
